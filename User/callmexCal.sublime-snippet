  <content><![CDATA[
int mexCal(vector<int>& a) {
     sort(a.begin(), a.end());
     int ans = 0;
     for (auto &it : a) {
          if (ans == it) {
               ans++;
          } else if (ans < it) {
               return ans;
          }
     }
     return ans;
}
]]></content>
